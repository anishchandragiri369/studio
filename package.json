{
  "name": "nextn",
  "version": "0.1.0",
  "private": true,  "scripts": {
    "dev": "next dev --turbopack -p 9002",
    "genkit:dev": "genkit start -- tsx src/ai/dev.ts",
    "genkit:watch": "genkit start -- tsx --watch src/ai/dev.ts",
    "build": "next build",
    "build:server": "npx cross-env MOBILE_BUILD=false STATIC_EXPORT=false NEXT_PUBLIC_BUILD_MODE=server next build",
    "build:static": "npm run static:prepare && npx cross-env MOBILE_BUILD=true NEXT_PUBLIC_BUILD_MODE=static next build && npm run static:restore",
    "start": "next start",
    "lint": "next lint",
    "typecheck": "tsc --noEmit",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "api:backup": "node scripts/build-utils.js backup",
    "api:prepare-static": "node scripts/build-utils.js prepare-static",
    "api:restore": "node scripts/build-utils.js restore",
    "api:cleanup": "node scripts/build-utils.js cleanup",
    "static:prepare": "node scripts/static-build.js prepare",
    "static:restore": "node scripts/static-build.js restore",
    "static:cleanup": "node scripts/static-build-utils.js cleanup",
    "cap:sync": "npm run build:static && npx cap sync",
    "cap:open:ios": "npx cap open ios",
    "cap:open:android": "npx cap open android",
    "mobile:dev": "npm run dev",
    "mobile:dev:android": "npx cap run android --livereload",
    "mobile:dev:ios": "npx cap run ios --livereload",
    "mobile:build": "npm run build:static && npx cap sync",
    "mobile:apk": "npm run mobile:build && npx cap run android",
    "apk:build": "node scripts/build-apk.js"
  },
